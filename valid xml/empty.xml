<?xml version="1.0" encoding="UTF-8"?>
<dcc:digitalCalibrationCertificate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://ptb.de/dcc https://ptb.de/dcc/v3.2.1/dcc.xsd" xmlns:dcc="https://ptb.de/dcc" xmlns:si="https://ptb.de/si" schemaVersion="3.2.1">
  <dcc:administrativeData>
    <dcc:dccSoftware>
      <dcc:software>
        <dcc:name>
          <dcc:content></dcc:content> <!-- (STATIC) This must show the Software Name of the DCC Generator -->
        </dcc:name>
        <dcc:release></dcc:release> <!-- This must show the version of the DCC Generator -->
      </dcc:software>
    </dcc:dccSoftware>
    <dcc:coreData>
      <dcc:countryCodeISO3166_1></dcc:countryCodeISO3166_1> <!-- (STATIC) This must show the Country Code of the calibration certificate origin -->
      <dcc:usedLangCodeISO639_1></dcc:usedLangCodeISO639_1> <!-- (STATIC) This must show the Language used in the calibration certificate -->
      <dcc:mandatoryLangCodeISO639_1></dcc:mandatoryLangCodeISO639_1> <!-- (STATIC) This must show the Language used in the calibration certificate -->
      <dcc:uniqueIdentifier></dcc:uniqueIdentifier> <!-- This must show the TSR Number used in the calibration certificate -->
      <dcc:beginPerformanceDate></dcc:beginPerformanceDate> <!-- This must show the Date of Calibration used in the calibration certificate -->
      <dcc:endPerformanceDate></dcc:endPerformanceDate> <!-- This must show the Date of Calibration used in the calibration certificate -->
      <dcc:performanceLocation></dcc:performanceLocation> <!-- (ONSITE or LABORATORY) This must show the place where the calibration was done -->
    </dcc:coreData>
    <dcc:items>
      <dcc:name>
        <dcc:content lang="en"></dcc:content> <!-- This must show the Calibration Item used in the calibration certificate -->
      </dcc:name>
      <dcc:equipmentClass refType=""> <!-- This must show the Name of Standard (pg2) of the equipment used in the calibration certificate -->
        <dcc:reference></dcc:reference> <!-- This must show the Name of Standard (pg2) of the equipment used in the calibration certificate -->
        <dcc:classID></dcc:classID> <!-- This must show the Make/Model of Standard (pg2) of the equipment used in the calibration certificate -->
      </dcc:equipmentClass>
      <dcc:item>
        <dcc:name>
          <dcc:content lang="en"></dcc:content> <!-- This must show the Calibration Item of the equipment used in the calibration certificate -->
        </dcc:name>
        <dcc:model></dcc:model> <!-- This must show the Make/Model of the equipment used in the calibration certificate -->
        <dcc:identifications>
          <dcc:identification>
            <dcc:issuer></dcc:issuer> <!-- This must show the Customer Name since they are the one being calibrated --> 
            <dcc:value></dcc:value> <!-- This must show the Serial No. of the equipment used in the calibration certificate -->
            <dcc:name>
              <dcc:content lang="en"></dcc:content> <!-- This must show the Make/model of the equipment being calibrated in the calibration certificate -->
            </dcc:name>
          </dcc:identification>
        </dcc:identifications>
      </dcc:item>
    </dcc:items>
    <dcc:calibrationLaboratory>
      <dcc:calibrationLaboratoryCode></dcc:calibrationLaboratoryCode> <!-- (STATIC) This must be in the settings; Use SAMPLE CODE for the optional values -->
      <dcc:contact>
        <dcc:name>
          <dcc:content lang="en"></dcc:content> <!-- (STATIC) This must be NML-ITDI unless changed -->
        </dcc:name>
        <dcc:location>
          <dcc:city></dcc:city> <!-- (STATIC) This must be Taguig unless relocated -->
          <dcc:countryCode></dcc:countryCode> <!-- (STATIC) This must be PH -->
          <dcc:postCode></dcc:postCode> <!-- (STATIC) This must be 1633 unless relocated -->
          <dcc:street></dcc:street> <!-- (STATIC) This must be General Santos Ave unless relocated -->
        </dcc:location>
      </dcc:contact>
    </dcc:calibrationLaboratory>
    <dcc:respPersons>
      <dcc:respPerson>
        <dcc:person>
          <dcc:name>
            <dcc:content lang="en"></dcc:content> <!-- This must be the First Science Research Specialist -->
          </dcc:name>
        </dcc:person>
        <dcc:role></dcc:role> <!-- This must be the First Science Research Specialist Role-->
      </dcc:respPerson>
      <dcc:respPerson>
        <dcc:person>
          <dcc:name>
            <dcc:content lang="en"></dcc:content> <!-- This must be the Second Science Research Specialist -->
          </dcc:name>
        </dcc:person>
        <dcc:role></dcc:role> <!-- This must be the Second Science Research Specialist Role-->
      </dcc:respPerson>
      <dcc:respPerson>
        <dcc:person>
          <dcc:name>
            <dcc:content lang="en"></dcc:content> <!-- This must be the Head of the Standards Section Name -->
          </dcc:name>
        </dcc:person>
        <dcc:role></dcc:role> <!-- This must be "Head, Pressure and Force Standards Section" -->
      </dcc:respPerson>
    </dcc:respPersons>
    <dcc:customer>
      <dcc:name>
        <dcc:content lang="en"></dcc:content> <!-- This must be the customer name -->
      </dcc:name>
      <dcc:location></dcc:location>  <!-- This must be the Address of the customer origin -->
    </dcc:customer>
  </dcc:administrativeData>
  <dcc:measurementResults>
  </dcc:measurementResults>
<dcc:comment></dcc:comment> <!-- This must include the Uncertainty of Measurement and the Remarks in the document appended -->
</dcc:digitalCalibrationCertificate>